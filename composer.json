{
    "name": "neurony/laravel-query-cache",
    "description": "Cache all {select} queries or only the duplicate ones for a specific Eloquent model",
    "license": "MIT",
    "keywords": [
        "laravel",
        "cache",
        "query",
        "model",
        "eloquent",
        "duplicate"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/support":"^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\QueryCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\QueryCache\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\QueryCache\\ServiceProvider"
            ]
        }
    }
}
